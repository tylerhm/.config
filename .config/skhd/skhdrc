# restart skhd
alt - r : skhd --restart-service

# app quick switcher
rcmd - e : open -a "/Applications/VS Code @ FB.app"
rcmd - t : open -a "/Applications/iTerm.app"
rcmd - b : open -a "/Applications/Google Chrome.app"
rcmd - c : open -a "/Applications/Workplace Chat.app"
rcmd - m : open -a "/Applications/Microsoft Outlook.app"
rcmd - s : open -a "/Applications/Spotify.app"
rcmd - f : open -a "/Applications/Flipper.app"
rcmd - h : open ~/
